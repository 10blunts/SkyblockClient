<Application x:Class="SkyblockClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="TextColor" Color="AntiqueWhite"/>
        <SolidColorBrush x:Key="LessDark" Color="#39385D"/>
        <SolidColorBrush x:Key="Dark" Color="#181735"/>
        <SolidColorBrush x:Key="Darker" Color="#0F0F2D"/>
        <SolidColorBrush x:Key="Red" Color="#FE6584"/>
        <SolidColorBrush x:Key="Reder" Color="#FF4C70"/>
    </Application.Resources>
</Application>
