<UserControl x:Class="SkyblockClient.ModDialControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SkyblockClient"
             mc:Ignorable="d" d:DesignWidth="200" Height="80">
    <Grid>
        <CheckBox x:Name="chkModEnabled" Content="CheckBox" Margin="10" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Click="ChkModEnabled_Click"/>

    </Grid>
</UserControl>
